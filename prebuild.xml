<?xml version="1.0" encoding="utf-8" ?>
<Prebuild xmlns="http://dnpb.sourceforge.net/schemas/prebuild-1.7.xsd" version="1.7">
  <Solution name="default" activeConfig="Debug" path="./" version="0.5.0-$Rev: 5161 $">
    <Configuration name="Debug">
      <Options>
        <CompilerDefines>TRACE;DEBUG</CompilerDefines>
        <OptimizeCode>false</OptimizeCode>
        <CheckUnderflowOverflow>false</CheckUnderflowOverflow>
        <AllowUnsafe>true</AllowUnsafe>
        <WarningLevel>4</WarningLevel>
        <WarningsAsErrors>false</WarningsAsErrors>
        <SuppressWarnings></SuppressWarnings>
        <OutputPath>bin\Debug\</OutputPath>
        <DebugInformation>true</DebugInformation>
        <IncrementalBuild>true</IncrementalBuild>
        <NoStdLib>false</NoStdLib>
      </Options>
    </Configuration>
    <Configuration name="Release">
      <Options>
        <CompilerDefines>TRACE</CompilerDefines>
        <OptimizeCode>true</OptimizeCode>
        <CheckUnderflowOverflow>false</CheckUnderflowOverflow>
        <AllowUnsafe>true</AllowUnsafe>
        <WarningLevel>4</WarningLevel>
        <WarningsAsErrors>false</WarningsAsErrors>
        <SuppressWarnings></SuppressWarnings>
        <OutputPath>bin\Release</OutputPath>
        <DebugInformation>false</DebugInformation>
        <IncrementalBuild>true</IncrementalBuild>
        <NoStdLib>false</NoStdLib>
      </Options>
    </Configuration>


    <Project name="OpenMetaverseTypes" path="OpenMetaverse/Types" type="Library">
       <Configuration name="Debug">
         <Options>
             <OutputPath>../../bin/</OutputPath>
         </Options>
       </Configuration>
       <Configuration name="Release">
         <Options>
             <OutputPath>../../bin/</OutputPath>
         </Options>
       </Configuration>

       <ReferencePath>../../bin/</ReferencePath>
       <Reference name="System" localCopy="false"/>
       <Reference name="System.Data" localCopy="false"/>
       <Reference name="System.Xml" localCopy="false"/>
       <Files>
         <Match pattern="*.cs" recurse="true"/>
       </Files>
     </Project>

        <Project name="OpenMetaverse" path="OpenMetaverse" type="Library">
      <Configuration name="Debug">
        <Options>
          <OutputPath>../bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Release">
        <Options>
          <OutputPath>../bin/</OutputPath>
        </Options>
      </Configuration>

      <ReferencePath>../bin/</ReferencePath>
      <Reference name="log4net" localCopy="false"/>
      <Reference name="System" localCopy="false"/>
      <Reference name="System.Drawing" localCopy="false"/>
      <Reference name="System.Data" localCopy="false"/>
      <Reference name="System.Xml" localCopy="false"/>
      <Reference name="OpenMetaverseTypes" localCopy="false"/>
      <Files>

        <Match pattern="*.cs" recurse="true">
          <Exclude name="Color4.cs"/>
          <Exclude name="Matrix4.cs"/>
          <Exclude name="Quaternion.cs"/>
          <Exclude name="Ray.cs"/>
          <Exclude name="Utils.cs"/>
          <Exclude name="UtilsConversions.cs"/>
          <Exclude name="UUID.cs"/>
          <Exclude name="Vector2.cs"/>
          <Exclude name="Vector3.cs"/>
          <Exclude name="Vector3d.cs"/>
          <Exclude name="Vector4.cs"/>
        </Match>
      </Files>
      </Project>




    <Project name="DotLisp" path="dotlisp" type="Library">
      <Configuration name="Debug">
        <Options>
          <OutputPath>../bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Release">
        <Options>
          <OutputPath>../bin/</OutputPath>
        </Options>
      </Configuration>

      <ReferencePath>../bin/</ReferencePath>
      <Reference name="System" localCopy="false"/>
      <Reference name="System.Data" localCopy="false"/>
      <Reference name="System.Xml" localCopy="false"/>
      <Files>
        <Match pattern="*.cs" recurse="false"/>
      </Files>
    </Project>

   <Project name="cogbot" path="." type="Exe">
      <Configuration name="Debug">
        <Options>
          <OutputPath>bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Release">
        <Options>
          <OutputPath>bin/</OutputPath>
        </Options>
      </Configuration>

      <ReferencePath>bin/</ReferencePath>
      <Reference name="log4net.dll" localCopy="true"/>
      <Reference name="System" localCopy="false"/>
      <Reference name="DotLisp.dll" localCopy="false"/>
      <Reference name="System.Drawing" localCopy="false"/>
      <Reference name="System.Windows.Forms" localCopy="false"/>
     <!-- <Reference name="System.Text" localCopy="false"/> -->
      <Reference name="System.Data" localCopy="false"/>
      <Reference name="System.Xml" localCopy="false"/>
      <Reference name="OpenMetaverseTypes" localCopy="false"/>
      <Reference name="OpenMetaverse.dll" localCopy="false"/>
      <Reference name="OpenCyc.dll" localCopy="false"/>
      <Reference name="IKVM.Runtime.dll" localCopy="false"/>
      <Reference name="IKVM.OpenJDK.ClassLibrary.dll" localCopy="false"/>
      <Files>
          <Match path="Actions" pattern="*.cs" recurse="true"/>
          <Match path="Listeners" pattern="*.cs" recurse="true"/>
          <Match path="Properties" pattern="*.cs" recurse="true"/>
          <Match path="Tutorials" pattern="*.cs" recurse="true"/>
          <Match path="Utilities" pattern="*.cs" recurse="true"/>
          <Match path="DotCYC" pattern="CycConnectionForm*.cs" recurse="true"/>
          <Match path="." pattern="*.cs" recurse="false"/>
      </Files>
    </Project>

  </Solution>
</Prebuild>
